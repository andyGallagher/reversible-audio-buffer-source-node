{
    "name": "reversible-audio-buffer-source-node",
    "version": "0.0.1",
    "description": "AudioBufferSourceNode variant that supports negative playback rate",
    "scripts": {
        "build": "DIST=$(pwd)/dist pnpm --dir=lib build",
        "dev": "pnpm --dir=dev dev",
        "format": "pnpm run --recursive --if-present --filter=!notifications --parallel format",
        "test": "vitest"
    },
    "homepage": "https://github.com/andyGallagher/reversible-audio-buffer-source-node",
    "keywords": [
        "reversible",
        "audio",
        "node",
        "buffer",
        "webaudio",
        "negative",
        "playback",
        "playbackrate"
    ],
    "author": "Andy Gallagher (https://www.andy-gallagher.com)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andyGallagher/reversible-audio-buffer-source-node.git"
    },
    "license": "MIT",
    "main": "index.js",
    "devDependencies": {
        "@types/node": "^22.10.5",
        "eslint": "^8.57.0",
        "eslint-plugin-no-relative-import-paths": "^1.6.1",
        "prettier": "^3.4.2",
        "tsx": "^4.19.2",
        "typescript": "~5.6.2",
        "vitest": "^3.0.2"
    },
    "type": "module"
}
